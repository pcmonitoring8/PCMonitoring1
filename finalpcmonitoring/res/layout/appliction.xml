<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/abc"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <Spinner
        android:id="@+id/MyMachineList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:entries="@array/machinename" />

    <Spinner
        android:id="@+id/ApplictionList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:entries="@array/ApplicationList" />

    <Button
        android:id="@+id/btnprocess_startkill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="doDisplayApplictionList"
        android:text="Perform" />

</TableLayout>
